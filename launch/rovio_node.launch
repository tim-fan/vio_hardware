<?xml version="1.0" encoding="UTF-8"?> 
<launch>
  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
    <param name="filter_config" value="$(find vio_hardware)/config/rovio.info"/>
    <param name="camera0_config" value="$(find vio_hardware)/config/rovio_cam0.yaml"/>
    <remap from="cam0/image_raw" to="/vio_hardware/image_timestamp_corrected"/>
    <remap from="imu0" to="/vio_hardware/imu"/>
  </node>
</launch>
